@model Department
@{
    ViewData["Title"] = "Update";
}

<h1>Update Department</h1>

<br />
<br />

<form asp-action="Update">

    <div class="form-group">
        <label asp-for="@Model.Code" class="col-form-label"></label>
        <input asp-for="@Model.Code" type="text" class="form-control" />
        <span asp-validation-for="Code" class="text-danger"></span>
    </div>


    <div class="form-group">
        <label asp-for="@Model.Name" class="col-form-label"></label>
        <input asp-for="@Model.Name" type="text" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>


    <div class="form-group">
        <label asp-for="@Model.DateOfCreation" class="col-form-label">Date Of Creation</label>
        <input asp-for="@Model.DateOfCreation" type="text" class="form-control" />
        <span asp-validation-for="DateOfCreation" class="text-danger"></span>
    </div>

    <br />
    <br />

    <input type="submit" value="Save" class="btn btn-primary" />
    <a href="Index">Back to List</a>




</form>
